@model IEnumerable<Vaskelista.Models.Room>

@{
    ViewBag.Title = "Index";
}

<h2>Romoversikt</h2>

<p>
    @Html.ActionLink("Legg til nytt rom", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Rediger", "Edit", new { id=item.RoomId }) |
            @Html.ActionLink("Detaljer", "Details", new { id=item.RoomId })
        </td>
    </tr>
}

</table>
